<table class="customers">
    <tbody> 
        {{#customers}}
        <tr> 
            <th colspan="2">{{fullName name}}</th>
        </tr>
        <tr>
            <td>e-mail</td> 
            <td><a href="mailto:{{email}}">{{email}}</a></td> 
        </tr> 
        <tr>
            <td>Secondary Phone</td> 
            <td>{{secondaryPhoneNumber}}</td> 
        </tr> 
        <tr>
            <td>CPR</td> 
            <td>{{cpr}}</a></td> 
        </tr> 
        <tr>
            <td valign=top>Address</td> 
            <td>{{address address}}</td> 
        </tr> 
        <tr>
            <td>Balance</td> 
            <td>{{balance}}</td> 
        </tr> 
        <tr>
            <td valign=top>Selective Services</td> 
            <td>{{selectiveServices selectiveServices}}</td> 
        </tr> 
        <tr>
            <td valign=top>Receive Promomotions Permissions</td> 
            <td>{{receivePromomotionsPermissions receivePromomotionsPermissions}}</td> 

        </tr> 
        {{/customers}} 
    </tbody> 
</table>
